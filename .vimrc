syntax on
syntax enable
set background=dark
set nu
let g:molokai_original = 1

let g:rehash256 = 1

colorscheme  molokai

set nu
set ruler
syntax on

set foldenable

set  encoding=utf-8

set fileencoding=utf-8

set completeopt=preview,menu
filetype plugin on

set langmenu=zh_CN.UTF-8

set tags=tags


if has('gui_running')
    set background=light
else
    set background=dark
endif




